@model BusinessObjects.Product
@{
    ViewData["Title"] = "Detail";
}
<h2>Detail</h2>

@if (Model != null)
{
    <div class="form-group">
        @Model.ProductId
    </div>
    <div class="form-group">
        @Model.ProductName
    </div>
    <div class="form-group">
        @Model.CategoryId
    </div>
    <div class="form-group">
        @Model.UnitPrice
    </div>
     <div class="form-group">
        @Model.UnitsInStock
    </div>
}